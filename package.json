{
  "name": "data-visualization-renderer",
  "displayName": "Data Visualization Renderer",
  "description": "Custom MIME renderer for data visualization",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/example/data-visualization-renderer.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "notebookRenderer": [
      {
        "id": "data-visualization-renderer",
        "displayName": "Data Visualization Renderer",
        "entrypoint": "./out/renderer.js",
        "mimeTypes": [
          "application/sagemaker-display"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npx tsc -p ./ && npx webpack --config webpack.config.js",
    "watch": "npx tsc -watch -p ./"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^4.9.5",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
